{
    "name": "swayok/peskycmf",
    "type": "library",
    "description": "PeskyCMF - CMF based on Laravel Framework 5, PeskyORM and AdminLTE UI template",
    "keywords": [
        "php",
        "cmf",
        "laravel",
        "admin",
        "database",
        "web"
    ],
    "authors": [
        {
            "name": "Alexander Filippov"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/swayok/PeskyCMF/issues"
    },
    "autoload": {
        "psr-0": {
            "PeskyCMF": "/src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5.3",
        "swayok/utils": "master@dev",
        "swayok/peskyorm": "v2.x-dev",
        "swayok/peskyorm-laravel": "master@dev",
        "swayok/laravel-extended-errors": "master@dev",
        "swayok/html-tag-helper": "master@dev",
        "almasaeed2010/adminlte": "2.4.*",
        "FortAwesome/Font-Awesome": "*",
        "driftyco/ionicons": "*",
        "nostalgiaz/bootstrap-switch": "*",
        "Eonasdan/bootstrap-datetimepicker": "*",
        "datatables/datatables": "*",
        "twbs/bootstrap": "3.*",
        "bower-asset/jquery": "3.*",
        "bower-asset/sortablejs": "~1.4",
        "swayok/page.js": "master@dev",
        "bootstrap-select/bootstrap-select": "*",
        "mistic100/jquery-querybuilder": "*",
        "grimmlink/toastr": "2.*",
        "moment/moment": "2.*",
        "jquery-form/form": "4.*",
        "select2/select2": "*",
        "robinherbots/jquery.inputmask": "~3.3",
        "bower-asset/cropper": "~2.3",
        "vitalets/x-editable": "dev-master",
        "kartik-v/bootstrap-fileinput": "~4.4.0",
        "vluzrmos/language-detector": "1.*"
    },
    "suggest": {
        "fxp/composer-asset-plugin": "Required to install bower/npm plugins!; composer global install fxp/composer-asset-plugin",
        "swayok/laravel-antihack-tool": "master@dev - active hack protection tool"
    }
}