{
    "name": "swayok/peskycmf",
    "type": "library",
    "description": "PeskyCMF - CMF based on Laravel Framework 5, PeskyORM and AdminLTE UI template",
    "keywords": [
        "php",
        "cmf",
        "laravel",
        "admin",
        "database",
        "web"
    ],
    "authors": [
        {
            "name": "Alexander Filippov"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/swayok/PeskyCMF/issues"
    },
    "autoload": {
        "psr-0": {
            "PeskyCMF": "/src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/swayok/peskyorm.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5.2",
        "swayok/utils": "~1.0",
        "swayok/peskyorm": "dev-v1_stable",
        "swayok/html-tag-helper": "~1.0",
        "swayok/laravel-site-loader": "master@dev",
        "almasaeed2010/adminlte": "2.3.*",
        "fortawesome/font-awesome": "^4.7",
        "driftyco/ionicons": "*",
        "nostalgiaz/bootstrap-switch": "*",
        "eonasdan/bootstrap-datetimepicker": "*",
        "datatables/datatables": "*",
        "ckeditor/ckeditor": "4.*",
        "twbs/bootstrap": "3.*",
        "components/jquery": "2.*",
        "snapappointments/bootstrap-select": "^1.13",
        "mistic100/jquery-querybuilder": "*",
        "grimmlink/toastr": "2.*",
        "moment/moment": "2.*",
        "jquery-form/form": "3.*",
        "afarkas/html5shiv": "*",
        "select2/select2": "*",
        "robinherbots/jquery.inputmask": "^3.3"
    }
}