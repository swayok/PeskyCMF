{
    "name": "swayok/peskycmf",
    "type": "library",
    "description": "PeskyCMF - CMF based on Laravel Framework 5, PeskyORM and AdminLTE UI template",
    "keywords": [
        "php",
        "cmf",
        "laravel",
        "admin",
        "database",
        "web"
    ],
    "authors": [
        {
            "name": "Alexander Filippov"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/swayok/PeskyCMF/issues"
    },
    "autoload": {
        "psr-0": {
            "PeskyCMF": "/src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "laravel/framework": ">=5.3",

        "swayok/utils": "master@dev",
        "swayok/peskyorm": "v2.x-dev",
        "swayok/peskyorm-laravel": "master@dev",
        "swayok/html-tag-helper": "master@dev",
        "vluzrmos/language-detector": "1.*",

        "almasaeed2010/adminlte": "2.4.*",
        "swayok/page.js": "master@dev",
        "mistic100/jquery-querybuilder": "2.*",

        "bower-asset/jquery": "3.*",
        "bower-asset/bootstrap": "3.*",
        "bower-asset/toastr": "2.*",
        "bower-asset/datatables": "1.*",
        "bower-asset/sortablejs": "1.*",
        "bower-asset/bootstrap-switch": "3.*",
        "bower-asset/eonasdan-bootstrap-datetimepicker": "4.*",
        "bower-asset/bootstrap-select": "1.*",
        "bower-asset/ajax-bootstrap-select": "1.*",
        "bower-asset/moment": "2.*",
        "bower-asset/jquery-form": "4.*",
        "bower-asset/select2": "4.*",
        "bower-asset/inputmask": "3.*",
        "bower-asset/bootstrap-fileinput": "4.4.*",
        "bower-asset/simple-scrollbar": "0.*"
    },
    "suggest": {
        "swayok/laravel-antihack-tool": "master@dev - active hack protection tool"
    },
    "todo": {
        "bower-asset/cropperjs": "1.*"
    }
}