{
    "name": "swayok/peskycmf",
    "type": "library",
    "description": "PeskyCMF - CMF based on Laravel Framework 5+, PeskyORM and AdminLTE 2 UI template",
    "keywords": [
        "php",
        "cmf",
        "laravel",
        "admin-ui",
        "database",
        "web",
        "scaffolding"
    ],
    "authors": [
        {
            "name": "Alexander Filippov"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/swayok/PeskyCMF/issues"
    },
    "autoload": {
        "psr-4": {
            "PeskyCMF\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.0",
        "laravel/framework": ">=5.3",
        "swayok/utils": "^1.7",
        "swayok/peskyorm-laravel": "dev-orm_v2_1",
        "swayok/html-tag-helper": "^1.0",
        "swayok/laravel-language-detector": "dev-master"
    },
    "suggest": {
        "swayok/laravel-antihack-tool": "master@dev - active hack protection tool",
        "npm or yarn": "Install node.js and yarn"
    }
}