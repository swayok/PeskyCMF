{
    "name": "swayok/peskycmf",
    "type": "library",
    "description": "PeskyCMF - CMF based on Laravel Framework 5+, PeskyORM and AdminLTE 2 UI template",
    "keywords": [
        "php",
        "cmf",
        "laravel",
        "admin-ui",
        "database",
        "web",
        "scaffolding"
    ],
    "authors": [
        {
            "name": "Alexander Filippov"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/swayok/PeskyCMF/issues"
    },
    "autoload": {
        "psr-4": {
            "PeskyCMF\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-imagick": "*",
        "ext-pdo": "*",
        "laravel/framework": ">=5.4",
        "swayok/utils": "^1.7.3",
        "swayok/peskyorm-laravel": "dev-orm_v3",
        "swayok/html-tag-helper": "^1.0"
    },
    "suggest": {
        "swayok/laravel-antihack-tool": "active hack protection tool (dev-master branch)",
        "swayok/alternative-laravel-cache": "alternative tagged and hierarchial cache drivers for Laravel",
        "swayok/laravel-extended-errors": "faster and more informative replacement for Laravel's Whoops",
        "npm or yarn": "Install node.js and yarn"
    }
}
